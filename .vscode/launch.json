{
  "version": "0.2.0",
  "configurations": [

    {
      "name": "gtest",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceRoot}/../../devel/lib/flatland_server/flatland_server_load_world_test",
      "args": ["--gtest_filter=FlatlandServerLoadWorldTest.world_invalid_C"],
      "stopAtEntry": false,
      "cwd": "${workspaceRoot}",
      "environment": [],
      "externalConsole": true,
      "MIMode": "gdb",
      "setupCommands": [
          {
              "description": "Enable pretty-printing for gdb",
              "text": "-enable-pretty-printing",
              "ignoreFailures": true
          }
      ]
    }
  ]
}